{
  "parser": "vue-eslint-parser",
  "parserOptions": { 
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "extends": [
    "plugin:vue/vue3-recommended",
    "maanex"
  ],
  "rules": {
    "vue/multi-word-component-names": "off",
    "vue/max-attributes-per-line": ["error", {
      "singleline": { "max": 6 },      
      "multiline": { "max": 1 }
    }],
    "vue/singleline-html-element-content-newline": "off"
  }
}
